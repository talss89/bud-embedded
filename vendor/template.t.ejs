<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta name=viewport content="width=device-width, initial-scale=1" />
  <title>%APP_TITLE%</title>
</head>

<body>
  <noscript>%NO_SCRIPT%</noscript>
  %%%DEPENDENCY_REQUIRES%%%
  %%%HTML_APP_BODY%%%
  <% if (htmlWebpackPlugin.options.isEmbeddedBuild) { %>
    <% htmlWebpackPlugin.files.css.forEach(function(cssFile) { 
      %>
      <style>
        <%=compilation.assets[cssFile.substr(htmlWebpackPlugin.files.publicPath.length)].source() %>
      </style>
      <% }); %>
        <% htmlWebpackPlugin.files.js.forEach(function(jsFile) { %>
          <script><%= compilation.assets[jsFile.substr(htmlWebpackPlugin.files.publicPath.length)].source() %></script>
          <% }); %>
            <% } %>
</body>
</html>